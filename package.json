{
  "name": "webpack",
  "publisher": "yuhengshen",
  "displayName": "webpack",
  "license": "MIT",
  "description": "",
  "version": "0.0.5",
  "repository": {
    "type": "github",
    "url": "https://github.com/yuhengshen/webpack"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "assets/icon-square-big.png",
  "activationEvents": [
    "onStartupFinished",
    "onCommand:extension.path"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.path",
        "title": "webpack"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "prettier": "prettier --write .",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "husky": "^4.3.6",
    "mocha": "^8.1.3",
    "prettier": "^2.2.1",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": "prettier --write"
  }
}
